{
  "name": "giveaway_system",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "sequelize": "^6.28.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "detectOpenHandles": true,
    "forceExit": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@models/(.*)$": "<rootDir>/src/models/$1",
      "^@commands/(.*)$": "<rootDir>/src/commands/$1",
      "^@tests/(.*)$": "<rootDir>/src/tests/$1"
    }
  },
  "devDependencies": {
    "@types/node": "^20.17.17",
    "@types/sequelize": "^4.28.20"
  }
}
